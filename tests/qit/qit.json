{
    "$schema": "https://qit.woo.com/json-schema/qit",
    "sut": {
        "type": "plugin",
        "slug": "poocommerce-payments",
        "source": {
            "type": "local",
            "path": "poocommerce-payments.zip"
        }
    },
    "environments": {
        "default": {
            "plugins": [
                { "slug": "poocommerce", "from": "wporg", "version": "stable" },
                { "slug": "jetpack", "from": "wporg", "version": "stable" }
            ]
        }
    },
    "test_types": {
        "e2e": {
            "default": {
                "environment": "default",
                "test_packages": [
                    "tests/qit/test-package"
                ]
            }
        }
    }
}
